his.isGoogleLogin = JSON.parse(localStorage.getItem('isLogin'));\n    this.fireAuth.getRedirectResult().then(result => {\n\n      // this.authenticate(result.user)\n      const user = result.user\n      var userData = {\n        profile: user.photoURL,\n        name: user.displayName,\n        email: user.email,\n        uid: user.uid\n      };\n     \n      this.router.navigate(['sign-up'], {\n        queryParams: {\n          user: JSON.stringify(userData)\n        }\n      });\n      \n    })\n  }\n\n  doLogin() {\n    if (this.platform.is('cordova') && this.platform.is('android')) {\n      this.google.login({}).then(user => {\n        var userData = {\n          profile: user.imageUrl,\n          name: user.displayName,\n          email: user.email,\n          uid: user.userId\n        };\n        // this.authenticate(user)\n        this.userService.getUserByEmail(user.email).subscribe(res => {\n          if(res !== null){\n            alert(JSON.stringify(userData));\n            localStorage.setItem('userData', JSON.stringify(userData) || '{}')\n            localStorage.setItem('isLogin', 'true');\n            // this.storage.set('userData', userData.toString());\n            // this.storage.set('isLogin', 'true');\n            this.router.navigate(['home']);\n          } else{\n            this.router.navigate(['sign-up'], {\n              queryParams: {\n                user: JSON.stringify(userData)\n              }\n            });\n          }\n        })\n      }).catch((error) => {\n          alert(error)\n        });\n\n    } else {\n\n      // this.fireAuth.signInWithRedirect(new firebase.auth.GoogleAuthProvider());\n      var userData = {\n        profile: 'user.imageUrl',\n        name:  'user.imageUrl',\n        email:  'user.imageUrl',\n        uid:  'user.imageUrl',\n      };\n\n      this.router.navigate(['sign-up'], {\n        queryParams: {\n          user: JSON.stringify(userData)\n        }\n      });\n    }\n  }\n\n  doLogout() {\n    this.fireAuth.signOut().then(() => {\n      localStorage.setItem('isLogin', 'false');\n      localStorage.removeItem('userData');\n      window.location.href = '/home';\n      this.isGoogleLogin = false;\n    });\n  }\n\n  // private authenticate(user) {\n  //   this.authService\n  //     .authenticate(user)\n  //     .subscribe(data => {\n  //       this.authService.setLocalStorage(data);\n  //       this.router.navigate(['/sign-up']);\n  //     });\n\n  //   this.authService.setLocalStorage(this.authService.authenticate(user));\n  //   this.router.navigate(['/sign-up']);\n  // }\n}"],"sourceRoot":"webpack:///"}