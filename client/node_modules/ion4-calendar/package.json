{
  "name": "ion4-calendar",
  "version": "3.0.4-beta.17",
  "description": "A date picker component for ionic4",
  "main_ivy_ngcc": "__ivy_ngcc__/dist/index.js",
  "main": "./dist/index.js",
  "typings": "./dist/index.d.ts",
  "scripts": {
    "start": "cd ./dev && ionic serve",
    "clean:dist": "rm -rf dist",
    "demo:link": "npm link && cd ./demo && npm link ion2-calendar",
    "demo:serve": "cd ./demo && ionic serve",
    "build": "run-s clean:dist build:copy-sources build:ts build:fix build:copy-scss",
    "build:ts": "tsc && ngc",
    "build:copy-sources": "gulp copy-sources",
    "build:copy-scss": "gulp copy-scss",
    "test": "echo \"Error: no test specified\"",
    "lint": "tslint -c tslint.json 'src/**/*.ts'",
    "packagr": "ng-packagr -p package.json",
    "build:fix": "find dist -maxdepth 1 -type f -delete && rm -r dist/services dist/components && mv dist/src/* dist/ && rm -r dist/src",
    "prepublishOnly": "node --eval \"console.error('ERROR: Trying to publish a package that has been compiled by NGCC. This is not allowed.\\nPlease delete and rebuild the package, without compiling with NGCC, before attempting to publish.\\nNote that NGCC may have been run by importing this package into another project that is being built with Ivy enabled.\\n')\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rift-walker/ion2-calendar.git"
  },
  "devDependencies": {
    "@angular-devkit/core": "^7.1.3",
    "@angular-devkit/schematics": "^7.1.3",
    "@angular/common": "7.1.3",
    "@angular/compiler": "7.1.3",
    "@angular/compiler-cli": "7.1.3",
    "@angular/core": "7.1.3",
    "@angular/forms": "7.1.3",
    "@angular/http": "7.1.3",
    "@angular/platform-browser": "7.1.3",
    "@angular/platform-browser-dynamic": "7.1.3",
    "@angular/router": "7.1.3",
    "@ionic/angular": "4.0.0-beta.18",
    "gulp": "^3.9.1",
    "gulp-bump": "^3.0.0",
    "ionicons": "~4.5.0",
    "ng-packagr": "^4.4.5",
    "npm-run-all": "^4.1.2",
    "rxjs": "6.3.3",
    "tsickle": "^0.34.0",
    "tslint": "~5.11.0",
    "tslint-eslint-rules": "^5.4.0",
    "typescript": "^3.1.6",
    "zone.js": "0.8.26"
  },
  "keywords": [
    "ionic4",
    "calendar",
    "Datepicker",
    "date-range"
  ],
  "author": "HsuanLee",
  "license": "MIT",
  "peerDependencies": {
    "moment": "^2.22.2"
  },
  "$schema": "./node_modules/ng-packagr/package.schema.json",
  "ngPackage": {
    "lib": {
      "entryFile": "src/index.ts",
      "umdModuleIds": {
        "moment": "moment"
      }
    }
  },
  "dependencies": {},
  "__processed_by_ivy_ngcc__": {
    "main": "10.0.14",
    "typings": "10.0.14"
  }
}
